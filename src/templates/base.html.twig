{% apply spaceless %}
<!DOCTYPE html>
<html>

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <link rel="shortcut icon" href="/public/favicon.png" type="image/x-icon">
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
    <link href="/public/bootstrap.min.5.0.0-beta1.css" rel="stylesheet" >
    <link rel="stylesheet" href="/public/dropzone.css">
    <link rel="stylesheet" href="/public/index.css">
    <title>{% block title %}{% endblock %}</title>
</head>

<body class="pb-3">
    {% if session_alert %}<div class="alert {{ session_alert.meaning }} alert-dismissible fade show m-3" role="alert">
        {{ session_alert.message|raw }}
        <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
    </div>{% endif %}
    {% block body %}{% endblock %}
    <script src="/public/bootstrap.bundle.min.5.0.0-beta1.js"></script>
    <script src="/public/dropzone.js"></script>
    <script src="/public/index.js"></script>
    <script>console.log('current user details :');console.log({{ current_user | json_encode | raw }})</script>
</body>

</html>
{% endapply %}